package com.bayviewglen.daytwo;

public class ExampleTwo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// declare 3 ints
		
		int numOne, numTwo, sum;
		
		numOne = 7;
		numTwo = 8;
		
		sum = numOne + numTwo;
		
		System.out.println(sum);
		
		System.out.println(numOne + " + " + numTwo + " = " + sum);
	}

}
